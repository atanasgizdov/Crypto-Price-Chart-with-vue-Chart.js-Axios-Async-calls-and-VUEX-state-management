scales: {\n          yAxes: [{\n            ticks: {\n              beginAtZero: true\n            }\n          }]\n        }\n      },\n\n      this.loaded = true\n      })\n      .catch(function (error) {\n        console.log(error)\n      })\n  }\n}\n</script>\n"],"sourceRoot":"src/components"}]}